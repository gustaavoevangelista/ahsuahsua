@model Final_Project_2._1.Models.Movimento

@{
    ViewData["Title"] = "AddConsumo";
}

<h1>Adicionar consumo</h1>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="AddConsumo">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>


            <div class="form-group">
                <label asp-for="ClienteId" class="control-label"></label>
                <input asp-for="ClienteId" class="form-control" />
                <span asp-validation-for="ClienteId" class="text-danger"></span>
            </div>

            <br />

            <div class="form-group">
                <label asp-for="Importancia" class="control-label"></label>
                <input asp-for="Importancia" class="form-control" />
                <span asp-validation-for="Importancia" class="text-danger"></span>
            </div>

            <br />

            <div class="form-group">
                <input type="submit" value="Adicionar" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>


<br />

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}

